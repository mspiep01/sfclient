<h1>I'm an edit page!</h1>

<div class="row">
	<div class="col-sm-2">
<%= form_for(@account, html: {role: "form"}) do |f| %>

<div class="form-group">
	<%= f.label :Name %>
	<%= f.text_field :Name, class: "form-control" %>
</div>

<div class="form-group" >
	<%= f.label "Street" %>
	<%= f.text_field :BillingStreet, class: "form-control" %>
</div>

<div class="form-group">
	<%= f.label "City" %>
	<%= f.text_field :BillingCity, class: "form-control" %>
</div>

<div class="form-group">
	<%= f.label "State" %>
	<%= f.text_field :BillingState, class: "form-control" %>
</div>

<div class="form-group">
	<%= f.label "Zip Code" %>
	<%= f.text_field :BillingPostalCode, class: "form-control" %>
</div>
<div class="form-group">
	<%= f.label "County" %>
	<%= f.text_field :County__c, class: "form-control" %>
</div>
<div class="form-group">
	<%= f.label "Municipality" %>
	<%= f.text_field :Municipality__c, class: "form-control" %>
</div>
</div>
<div class="col-sm-2 col-sm-offset-3">
<%= f.submit "Save Changes", class: "btn btn-primary" %>
<% end %>
</div>
<%= debug(params) if Rails.env.development? %>
</div>
